# Server settings
PORT=8081
HOST=0.0.0.0

# API keys - DO NOT INCLUDE ACTUAL KEYS IN VERSION CONTROL
# Configure these in your deployment environment (Render/Vercel)
ANTHROPIC_API_KEY=your-anthropic-api-key

# Model settings
CLAUDE_MODEL=claude-3-5-sonnet-20241022

# Database settings
USE_MEMORY_DB=false

# Supabase connection details - DO NOT INCLUDE ACTUAL KEYS IN VERSION CONTROL
# Configure these in your deployment environment (Render/Vercel)
SUPABASE_URL=your-supabase-url
SUPABASE_KEY=your-supabase-key
SUPABASE_SERVICE_KEY=your-supabase-service-key
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# LLM API Configuration - IMPORTANT: Use ANTHROPIC_API_KEY instead of CLAUDE_API_KEY
# The variable CLAUDE_API_KEY is deprecated and no longer used

# App settings
DEBUG=false

# Keep alive settings
ENABLE_WARMUP=true 