# Server settings
PORT=8081
HOST=0.0.0.0

# API keys - Do not commit actual keys to version control
# Configure these in your Render dashboard
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Model settings
CLAUDE_MODEL=claude-3-5-sonnet-20241022

# Database settings
USE_MEMORY_DB=false

# Supabase connection details - Do not commit actual values to version control
# Configure these in your Render dashboard
SUPABASE_URL=your-supabase-url-here
SUPABASE_KEY=your-supabase-key-here
SUPABASE_SERVICE_KEY=your-supabase-service-key-here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# LLM API Configuration - IMPORTANT: Use ANTHROPIC_API_KEY instead of CLAUDE_API_KEY
# The variable CLAUDE_API_KEY is deprecated and no longer used

# App settings
DEBUG=false

# Keep alive settings
ENABLE_WARMUP=true 